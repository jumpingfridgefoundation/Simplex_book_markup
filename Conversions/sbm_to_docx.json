      "sbm_paragraph": {
        "docx_element": "w:p",
        "docx_schema": "wordprocessingml",
        "paragraph_style": "Normal",
        "xml_structure": "<w:p><w:r><w:t>{content}</w:t></w:r></w:p>",
        "notes": "Standard paragraph with text content"
      }
