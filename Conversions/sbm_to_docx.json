{
  "schema_version": "1.0",
  "source_language": "SBM",
  "target_language": "DOCX",
  "description": "SimplexBook Markup to Microsoft Word DOCX conversion mapping using Office Open XML",
  "docx_standards": {
    "format": "Office Open XML (ISO/IEC 29500)",
    "file_extension": ".docx",
    "mime_type": "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
    "compression": "ZIP-based container"
  },
  "element_mappings": {
