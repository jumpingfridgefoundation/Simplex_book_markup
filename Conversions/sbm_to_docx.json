      "sbm_document": {
        "docx_element": "w:document",
        "docx_schema": "wordprocessingml",
        "namespace": "http://schemas.openxmlformats.org/wordprocessingml/2006/main",
        "xml_structure": "<w:document xmlns:w=\"http://schemas.openxmlformats.org/wordprocessingml/2006/main\">\n  <w:body>\n    {content}\n  </w:body>\n</w:document>",
        "required_properties": "Document properties and metadata",
        "accessibility_features": "Word accessibility tags and navigation"
      },
      "sbm_book": {
        "docx_element": "w:body",
        "docx_schema": "wordprocessingml",
        "xml_structure": "Main document body with paragraphs and sections",
        "document_properties": "Title, author, subject metadata",
        "notes": "DOCX doesn't have explicit book structure, uses document properties"
      },
      "sbm_chapter": {
        "docx_element": "w:p",
        "docx_schema": "wordprocessingml",
        "paragraph_style": "Heading 1",
        "xml_structure": "<w:p><w:pPr><w:pStyle w:val=\"Heading1\"/></w:pPr><w:r><w:t>{title}</w:t></w:r></w:p>",
        "notes": "Chapters map to Heading 1 style paragraphs"
      },
      "sbm_section": {
        "docx_element": "w:p",
        "docx_schema": "wordprocessingml",
