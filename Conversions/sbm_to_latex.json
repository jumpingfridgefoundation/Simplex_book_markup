{
  "schema_version": "1.0",
  "source_language": "SBM",
  "target_language": "LaTeX",
  "description": "SimplexBook Markup to LaTeX conversion mapping for academic and technical documents",
  "latex_standards": {
    "latex_version": "LaTeX2e",
    "recommended_packages": [
      "amsmath",
      "amssymb",
      "amsthm",
      "geometry",
      "fancyhdr",
      "hyperref",
      "inputenc",
      "babel",
      "graphicx",
      "tabularx"
    ],
    "document_classes": [
      "article",
      "book",
      "report",
      "scrarticle",
      "scrbook"
    ]
  },
  "element_mappings": {
    "document_structure": {
      "sbm_book": {
        "latex_syntax": "\\begin{document}\\n{content}\\n\\end{document}",
        "preamble_required": [
          "\\documentclass[12pt]{book}",
          "\\usepackage[utf8]{inputenc}",
          "\\usepackage[english]{babel}"
        ],
        "notes": "Books use book document class with proper chapter structure",
        "alternatives": {
          "report": "\\documentclass[12pt]{report}",
          "article": "\\documentclass[12pt]{article}"
        }
      },
      "sbm_chapter": {
        "latex_syntax": "\\chapter{{{title}}}",
        "notes": "Chapter command with title parameter",
        "book_class_only": true,
        "alternatives": {
          "article": "\\section{{{title}}}",
          "report": "\\chapter{{{title}}}"
        }
      },
      "sbm_section": {
        "latex_syntax": "\\section{{{title}}}",
        "notes": "Standard section command",
        "hierarchical_structure": {
          "sbm_h1": "\\chapter{{{title}}}",
          "sbm_h2": "\\section{{{title}}}",
          "sbm_h3": "\\subsection{{{title}}}",
          "sbm_h4": "\\subsubsection{{{title}}}",
          "sbm_h5": "\\paragraph{{{title}}}",
          "sbm_h6": "\\subparagraph{{{title}}}"
        }
      },
      "sbm_paragraph": {
        "latex_syntax": "{content}\\par",
        "notes": "Paragraph content with explicit paragraph break",
        "whitespace_handling": "Multiple spaces preserved with \\hspace*{}"
      }
    },
    "text_formatting": {
      "sbm_bold": {
        "latex_syntax": "\\textbf{{{text}}}",
        "notes": "Bold text with textbf command",
        "alternative_syntax": "{\\bf {text}}",
        "math_mode": "\\mathbf{{{text}}}"
      },
      "sbm_italic": {
        "latex_syntax": "\\textit{{{text}}}",
        "notes": "Italic text with textit command",
        "alternative_syntax": "{\\it {text}}",
        "math_mode": "\\mathit{{{text}}}"
      },
      "sbm_underline": {
        "latex_syntax": "\\underline{{{text}}}",
        "notes": "Underlined text with uline command",
        "package_required": "soul"
      },
      "sbm_code": {
        "latex_syntax": "\\texttt{{{code}}}",
        "notes": "Monospace font for inline code",
        "package_required": "listings",
        "alternative_syntax": "{\\tt {code}}"
      },
      "sbm_code_block": {
        "latex_syntax": "\\begin{verbatim}\\n{code}\\n\\end{verbatim}",
        "notes": "Verbatim environment for code blocks",
        "package_required": "listings",
        "enhanced_syntax": "\\begin{verbatim}[fontsize=\\small]\\n{code}\\n\\end{verbatim}",
        "syntax_highlighting": "\\lstset{language={language}}\\n\\begin{{{language}}}[caption={caption}]\\n{code}\\n\\end{{{language}}}"
      },
      "sbm_superscript": {
        "latex_syntax": "{text}^{superscript}",
        "notes": "Superscript in math mode",
        "text_mode": "\\textsuperscript{{{text}}}",
        "package_required": "textcomp"
      },
      "sbm_subscript": {
        "latex_syntax": "{text}_{subscript}",
        "notes": "Subscript in math mode",
        "text_mode": "\\textsubscript{{{text}}}",
        "package_required": "textcomp"
      },
      "sbm_strikethrough": {
        "latex_syntax": "\\sout{{{text}}}",
        "notes": "Strikethrough text",
        "package_required": "soul"
      },
      "sbm_small_caps": {
        "latex_syntax": "\\scshape{{{text}}}",
        "notes": "Small caps for abbreviations",
        "alternative_syntax": "{\\sc {text}}"
      }
    },
    "lists": {
      "sbm_ordered_list": {
        "latex_syntax": "\\begin{enumerate}\\n\\item {item1}\\n\\item {item2}\\n\\end{enumerate}",
        "notes": "Numbered list with enumerate environment",
        "nested_structure": "\\begin{enumerate}\\n\\item {item}\\n\\begin{enumerate}\\n\\item {subitem}\\n\\end{enumerate}\\n\\end{enumerate}",
        "customization": {
          "roman_numbers": "\\begin{enumerate}[label=\\roman*]",
          "letters": "\\begin{enumerate}[label=\\alph*]",
          "custom_format": "\\begin{enumerate}[label={Item \\arabic*:}]"
        }
      },
      "sbm_unordered_list": {
        "latex_syntax": "\\begin{itemize}\\n\\item {item1}\\n\\item {item2}\\n\\end{itemize}",
        "notes": "Bulleted list with itemize environment",
        "nested_structure": "\\begin{itemize}\\n\\item {item}\\n\\begin{itemize}\\n\\item {subitem}\\n\\end{itemize}\\n\\end{itemize}",
        "custom_bullets": "\\renewcommand{\\labelitemi}{\\textbullet}"
      },
      "sbm_list_item": {
        "latex_syntax": "\\item {content}",
        "notes": "List item with content",
        "enumeration_styles": {
          "numeric": "\\arabic*",
          "roman": "\\roman*",
          "alphabetic": "\\alph*"
        }
      },
      "sbm_definition_list": {
        "latex_syntax": "\\begin{description}\\n\\item[{term}] {definition}\\n\\end{description}",
        "notes": "Definition list with description environment",
        "package_required": "enumitem",
        "enhanced_formatting": "\\begin{description}[style=nextline,leftmargin=4em]\\n\\item[{term}] {definition}\\n\\end{description}"
      },
      "sbm_definition_term": {
        "latex_syntax": "\\item[{term}]",
        "notes": "Term in description environment",
        "bold_term": "\\item[\\textbf{{{term}}}]"
      },
      "sbm_definition": {
        "latex_syntax": "{definition}",
        "notes": "Definition following term"
      }
    },
    "links_and_references": {
      "sbm_link": {
        "latex_syntax": "\\href{{{url}}}{{text}}",
        "notes": "Hyperlinked text with href command",
        "package_required": "hyperref",
        "display_url": "\\url{{{url}}}",
        "simple_link": "\\nolinkurl{{{url}}}"
      },
      "sbm_internal_link": {
        "latex_syntax": "\\hyperref[{id}]{{{text}}}",
        "notes": "Internal cross-reference link",
        "package_required": "hyperref",
        "creation": "\\label{{{id}}}"
      },
      "sbm_citation": {
        "latex_syntax": "\\cite{{{reference_id}}}",
        "notes": "Citation command with reference key",
        "package_required": "natbib or biblatex",
        "multiple_citations": "\\cite{{{ref1,ref2,ref3}}}",
        "page_reference": "\\cite[p.~{page}]{{{reference_id}}}",
        "text_citation": "\\citet{{{reference_id}}}",
        "parenthetical": "\\citep{{{reference_id}}}"
      }
    },
    "tables": {
      "sbm_table": {
        "latex_syntax": "\\begin{table}[htbp]\\n\\centering\\n\\begin{tabular}{|c|c|c|}\\n\\hline\\n{content}\\n\\hline\\n\\end{tabular}\\n\\caption{{{caption}}}\\n\\label{{{label}}}\\n\\end{table}",
        "notes": "Floating table environment with caption and label",
        "package_required": "booktabs for professional tables",
        "simple_table": "\\begin{tabular}{|c|c|c|}\\n\\hline\\n{content}\\n\\hline\\n\\end{tabular}",
        "professional_style": "\\begin{tabular}{ccc}\\n\\toprule\\n{content}\\n\\bottomrule\\n\\end{tabular}"
      },
      "sbm_table_caption": {
        "latex_syntax": "\\caption{{{caption}}}",
        "notes": "Table caption command",
        "short_caption": "\\caption[Short caption]{{{long_caption}}}",
        "package_required": "caption for formatting"
      },
      "sbm_table_row": {
        "latex_syntax": "{cell1} & {cell2} & {cell3} \\\\",
        "notes": "Row with ampersand separators and line break",
        "header_row": "\\hline\\n{cell1} & {cell2} & {cell3} \\\\ \\hline"
      },
      "sbm_table_header": {
        "latex_syntax": "\\textbf{{{header_text}}}",
        "notes": "Bold headers for table headers",
        "multirow": "\\multirow{{{rows}}}{{*}}{{{text}}}",
        "package_required": "multirow"
      },
      "sbm_table_cell": {
        "latex_syntax": "{content}",
        "notes": "Table cell content",
        "alignment": {
          "left": "l",
          "center": "c",
          "right": "r"
        }
      },
      "sbm_table_column": {
        "latex_syntax": "column_specifier",
        "notes": "Column specification in tabular environment",
        "specifiers": {
          "left": "l",
          "center": "c",
          "right": "r",
          "paragraph": "p{width}",
          "vertical_line": "|",
          "double_line": "||"
        }
      }
    },
    "images_and_media": {
      "sbm_image": {
        "latex_syntax": "\\includegraphics[width={width}]{path/to/image}",
        "notes": "Image inclusion with optional width specification",
        "package_required": "graphicx",
        "full_options": "\\includegraphics[width={width},height={height},keepaspectratio]{image}",
        "scaling": {
          "scale": "\\includegraphics[scale={factor}]{image}",
          "width": "\\includegraphics[width={width}]{image}",
          "height": "\\includegraphics[height={height}]{image}"
        }
      },
      "sbm_figure": {
        "latex_syntax": "\\begin{figure}[htbp]\\n\\centering\\n\\includegraphics{image}\\n\\caption{{{caption}}}\\n\\label{{{label}}}\\n\\end{figure}",
        "notes": "Floating figure environment for images",
        "package_required": "graphicx",
        "multiple_images": "\\subfigure[caption A]{\\includegraphics{imageA}}\\n\\subfigure[caption B]{\\includegraphics{imageB}}",
        "package_required_subfigure": "subfig"
      },
      "sbm_figure_caption": {
        "latex_syntax": "\\caption{{{caption}}}",
        "notes": "Figure caption command",
        "short_caption": "\\caption[Short]{{{long_caption}}}"
      },
      "sbm_audio": {
        "latex_syntax": "\\href{run:audiofile.mp3}{Audio Link}",
        "notes": "Link to audio file (PDF viewer dependent)",
        "package_required": "hyperref",
        "media_package": "\\usepackage{media9}",
        "embed_audio": "\\includemedia[width={width}]{audio.mp3}{altaudio.mp3}"
      },
      "sbm_video": {
        "latex_syntax": "\\includemedia[width={width},height={height}]{video.mp4}{poster.jpg}",
        "notes": "Embedded video in PDF",
        "package_required": "media9",
        "fallback": "\\href{run:video.mp4}{Video Link}"
      }
    },
    "mathematical_content": {
      "sbm_math": {
        "latex_syntax": "${math_expression}$",
        "notes": "Inline mathematics with single dollar signs",
        "display_mode": "$${math_expression}$$",
        "equation_environment": "\\begin{equation}\\n{math_expression}\\n\\end{equation}",
        "package_required": "amsmath for advanced math"
      },
      "sbm_formula": {
        "latex_syntax": "\\begin{equation}\\n{formula}\\n\\label{{{label}}}\\n\\end{equation}",
        "notes": "Numbered equation with label for references",
        "references": "\\eqref{{{label}}}",
        "multi_line": "\\begin{align}\\n{line1} \\\\\\n{line2}\\n\\end{align}",
        "package_required": "amsmath"
      },
      "sbm_math_inline": {
        "latex_syntax": "${{{math}}}$",
        "notes": "Inline mathematical expression",
        "fraction": "$\\frac{numerator}{denominator}$",
        "sqrt": "$\\sqrt{expression}$",
        "subscript": "$x_{subscript}$",
        "superscript": "$x^{superscript}$"
      },
      "sbm_math_display": {
        "latex_syntax": "$${display_expression}$$",
        "notes": "Display mode mathematics",
        "equation_environment": "\\begin{equation}\\n{display_expression}\\n\\end{equation}",
        "align_environment": "\\begin{align}\\n{align_expression}\\n\\end{align}"
      }
    },
    "block_elements": {
      "sbm_blockquote": {
        "latex_syntax": "\\begin{quote}\\n{blockquote}\\n\\end{quote}",
        "notes": "Quote environment for indented text",
        "long_quote": "\\begin{quotation}\\n{long_quote}\\n\\end{quotation}",
        "citation_in_quote": "\\begin{quote}\\n{text}\\n\\raggedleft--- \\textit{{{author}}}\\n\\end{quote}"
      },
      "sbm_horizontal_rule": {
        "latex_syntax": "\\hline",
        "notes": "Horizontal line in tables",
        "thick_line": "\\hline\\n\\hline",
        "custom_line": "\\rule{\\textwidth}{0.5pt}",
        "vertical_spacing": "\\vspace{1em}\\n\\hline\\n\\vspace{1em}"
      },
      "sbm_code_block": {
        "latex_syntax": "\\begin{verbatim}\\n{code}\\n\\end{verbatim}",
        "notes": "Verbatim environment for code blocks",
        "package_required": "listings",
        "enhanced_code": "\\begin{{{language}}[caption={caption}]\\n{code}\\n\\end{{{language}}}",
        "inline_code": "\\lstinline[fontadjust]{code}"
      },
      "sbm_note": {
        "latex_syntax": "\\begin{mdframed}[style=note]\\n{note}\\n\\end{mdframed}",
        "notes": "Highlighted note boxes",
        "package_required": "mdframed",
        "simple_note": "\\textcolor{blue}{\\textbf{Note: }} {text}",
        "packages_required": "xcolor"
      }
    },
    "references_and_citations": {
      "sbm_bibliography": {
        "latex_syntax": "\\bibliography{database_name}",
        "notes": "Bibliography database reference",
        "bibliography_style": "\\bibliographystyle{{{style}}}",
        "bibtex_backend": "Requires .bib file with references",
        "biblatex_backend": "\\printbibliography"
      },
      "sbm_footnote": {
        "latex_syntax": "text\\footnote{{{footnote_text}}}",
        "notes": "Inline footnote",
        "numbered_footnotes": "Automatic numbering",
        "symbol_footnotes": "\\renewcommand{\\thefootnote}{\\fnsymbol{footnote}}",
        "multiple_footnotes": "\\footnote[number]{{{text}}}"
      },
      "sbm_index": {
        "latex_syntax": "\\index{{{entry}}}",
        "notes": "Index entry for searchable index",
        "package_required": "makeidx",
        "index_processing": "Requires makeindex compilation",
        "index_style": "\\index{{{entry}|textbf}",
        "page_range": "\\index{{{entry}|see{also_entry}}}"
      },
      "sbm_glossary": {
        "latex_syntax": "\\newglossaryentry{{{name}}}{{name={{{name}}}, description={{{description}}}}}",
        "notes": "Glossary entry definition",
        "package_required": "glossaries",
        "glossary_reference": "\\gls{{{name}}}",
        "acronym": "\\newacronym[description={{{description}}}]{{{short}}}{{{{long}}}",
        "acronym_usage": "\\acrlong{{{short}}} or \\acrshort{{{short}}}"
      }
    },
    "document_metadata": {
      "sbm_title": {
        "latex_syntax": "\\title{{{title}}}",
        "notes": "Document title for title page",
        "subtitle": "\\subtitle{{{subtitle}}}",
        "author": "\\author{{{author}}}",
        "date": "\\date{{{date}}}",
        "title_page": "\\maketitle"
      },
      "sbm_abstract": {
        "latex_syntax": "\\begin{abstract}\\n{content}\\n\\end{abstract}",
        "notes": "Abstract environment for summary",
        "keywords": "\\begin{abstract}\\n{content}\\n\\textbf{Keywords:} {keywords}\\n\\end{abstract}",
        "book_class_abstract": "\\begin{titlepage}\\n\\begin{abstract}\\n{content}\\n\\end{abstract}\\n\\end{titlepage}"
      },
      "sbm_keywords": {
        "latex_syntax": "\\textbf{Keywords:} {keyword1}, {keyword2}, {keyword3}",
        "notes": "Keywords section",
        "alternative_format": "\\subsection*{Keywords}\\n{keyword1}, {keyword2}, {keyword3}"
      },
      "sbm_author": {
        "latex_syntax": "\\author{{{name}}}",
        "notes": "Author information",
        "multiple_authors": "\\author{{{name1}} \\\\ {name2}}",
        "affiliation": "\\author{{{name}}\\\\ {affiliation}}",
        "email": "\\author{{{name}}\\\\ \\email{{{email}}}",
        "package_required": "hyperref for email links"
      }
    },
    "custom_commands": {
      "sbm_new_command": {
        "latex_syntax": "\\newcommand{{\\command}}[{{number_of_args}}]{{{{definition}}}}",
        "notes": "Define custom commands",
        "example": "\\newcommand{\\sbmstrong}[1]{\\textbf{#1}}",
        "with_options": "\\newcommand{\\sbmheading}[2][default]{\\textbf{#2 (#1)}}",
        "redefine": "\\renewcommand{{\\command}}[{{number}}]{{{{new_definition}}}}"
      },
      "sbm_new_environment": {
        "latex_syntax": "\\newenvironment{{name}}[{{num_args}}]{{{{begin_def}}}} {{{end_def}}}}",
        "notes": "Define custom environments",
        "example": "\\newenvironment{sbmquote}{\\begin{quote}}{\\end{quote}}",
        "with_parameters": "\\newenvironment{sbmwarning}[1]{\\begin{trivlist}\\n\\item[\\hspace{-\\labelsep}\\textbf{Warning: #1}]}{\\end{trivlist}}"
      }
    },
    "advanced_features": {
      "sbm_hyperlink": {
        "latex_syntax": "\\hyperref[{label}]{{{text}}}",
        "notes": "Internal hyperlinks",
        "package_required": "hyperref",
        "external_link": "\\href{{{url}}}{{text}}",
        "email_link": "\\hrefmailto{{{email}}}",
        "file_link": "\\href{run:file.pdf}{File Link}"
      },
      "sbm_bookmark": {
        "latex_syntax": "\\pdfbookmark[{{level}}]{{{text}}}{{{name}}}",
        "notes": "PDF bookmarks for navigation",
        "package_required": "hyperref",
        "chapter_bookmark": "\\chapter{{{title}}}\\n\\pdfbookmark[1]{{{title}}}{{chap:title}}"
      },
      "sbm_color": {
        "latex_syntax": "\\textcolor{{{color}}}{{{text}}}",
        "notes": "Colored text",
        "package_required": "xcolor",
        "colorbox": "\\colorbox{{{color}}}{{{text}}}",
        "background_color": "\\pagecolor{{{color}}}",
        "define_color": "\\definecolor{{{name}}}{rgb}{{r,g,b}}"
      }
    }
  },
  "package_recommendations": {
    "essential_packages": {
      "geometry": {
        "purpose": "Page layout control",
        "usage": "\\usepackage[margin=1in]{geometry}"
      },
      "fancyhdr": {
        "purpose": "Header and footer customization",
        "usage": "\\usepackage{fancyhdr}"
      },
      "hyperref": {
        "purpose": "Hyperlinks and navigation",
        "usage": "\\usepackage[colorlinks=true]{hyperref}"
      },
      "graphicx": {
        "purpose": "Image inclusion",
        "usage": "\\usepackage{graphicx}"
      },
      "amsmath": {
        "purpose": "Advanced mathematical typesetting",
        "usage": "\\usepackage{amsmath}"
      }
    },
    "optional_packages": {
      "listings": {
        "purpose": "Syntax highlighting for code",
        "usage": "\\usepackage[language=python]{listings}"
      },
      "booktabs": {
        "purpose": "Professional table formatting",
        "usage": "\\usepackage{booktabs}"
      },
      "enumitem": {
        "purpose": "List customization",
        "usage": "\\usepackage{enumitem}"
      },
      "babel": {
        "purpose": "Language support and hyphenation",
        "usage": "\\usepackage[english]{babel}"
      },
      "glossaries": {
        "purpose": "Glossary and index generation",
        "usage": "\\usepackage{glossaries}"
      }
    }
  },
  "conversion_rules": {
    "attribute_handling": {
      "title_conversion": {
        "sbm_attr": "title",
        "latex_usage": "\\title{{{title}}} or section command parameter",
        "special_characters": "Use \\\\ for LaTeX special characters"
      },
      "id_conversion": {
        "sbm_attr": "id",
        "latex_usage": "\\label{{{id}}}",
        "validation": "Must be unique within document"
      },
      "lang_conversion": {
        "sbm_attr": "lang",
        "latex_usage": "babel package option",
        "examples": {
          "en": "babel",
          "fr": "\\usepackage[french]{babel}",
          "de": "\\usepackage[ngerman]{babel}"
        }
      }
    },
    "content_transformation": {
      "special_characters": {
        "latex_reserved": ["#", "$", "%", "&", "~", "_", "^", "\\", "{", "}"],
        "escape_method": "prefix with backslash",
        "examples": {
          "\\#": "#",
          "\\$": "$",
          "\\%": "%",
          "\\&": "&",
          "\\_": "_",
          "\\^{}": "^",
          "\\~{}": "~",
          "\\textbackslash": "\\",
          "\\{": "{",
          "\\}": "}"
        }
      },
      "whitespace_handling": {
        "preserve_spaces": "\\hspace*{spacing}",
        "non_breaking_spaces": "~",
        "multiple_spaces": "\\quad or \\qquad",
        "line_breaks": "\\\\ or \\par"
      },
      "math_mode_requirements": {
        "inline_math": "Single $ signs",
        "display_math": "Double $$ or equation environment",
        "special_math_commands": "amsmath package recommended"
      }
    }
  },
  "document_classes": {
    "article": {
      "purpose": "Articles, reports, and short documents",
      "structure": "sections and subsections",
      "usage": "\\documentclass[options]{article}"
    },
    "book": {
      "purpose": "Books and long documents",
      "structure": "chapters, parts, and appendices",
      "usage": "\\documentclass[options]{book}"
    },
    "report": {
      "purpose": "Longer reports and dissertations",
      "structure": "chapters and sections",
      "usage": "\\documentclass[options]{report}"
    }
  },
  "example_conversions": {
    "simple_article": {
      "sbm_code": "::article title=\"LaTeX Conversion\" lang=\"en\"\n::author name=\"Author Name\"\n::section title=\"Introduction\"\n::p This is an introduction with *emphasis* and **bold** text.\n::ul\n::li First item\n::li Second item\n::/ul\n::section title=\"Code Example\"\n::code_block\nprint(\"Hello, World!\")\n::/code_block\n::/article",
      "latex_output": "\\documentclass[12pt]{article}\\n\\usepackage[utf8]{inputenc}\\n\\usepackage[english]{babel}\\n\\usepackage{hyperref}\\n\\n\\title{LaTeX Conversion}\\n\\author{Author Name}\\n\\n\\begin{document}\\n\\maketitle\\n\\n\\section{Introduction}\\nThis is an introduction with \\textit{emphasis} and \\textbf{bold} text.\\n\\begin{itemize}\\n\\item First item\\n\\item Second item\\n\\end{itemize}\\n\\n\\section{Code Example}\\n\\begin{verbatim}\\nprint(\\"Hello, World!\\")\\n\\end{verbatim}\\n\\n\\end{document}"
    },
    "mathematical_document": {
      "sbm_code": "::report title=\"Mathematical Analysis\" lang=\"en\"\n::chapter title=\"Calculations\"\n::section title=\"Derivatives\"\n::p The derivative of $f(x) = x^2$ is $f'(x) = 2x$.\n::formula label=\"derivative\"\n\\frac{d}{dx} x^n = nx^{n-1}\n::/formula\n::section title=\"Integration\"\n::p Consider the integral $\\\\int_0^1 x^2 dx = \\\\frac{1}{3}$.\n::/chapter\n::/report",
      "latex_output": "\\documentclass[12pt]{report}\\n\\usepackage[utf8]{inputenc}\\n\\usepackage[english]{babel}\\n\\usepackage{amsmath}\\n\\usepackage{hyperref}\\n\\n\\title{Mathematical Analysis}\\n\\n\\begin{document}\\n\\maketitle\\n\\n\\chapter{Calculations}\\n\\section{Derivatives}\\nThe derivative of $f(x) = x^2$ is $f'(x) = 2x$.\\n\\begin{equation}\\n\\frac{d}{dx} x^n = nx^{n-1}\\n\\label{derivative}\\n\\end{equation}\\n\\section{Integration}\\nConsider the integral $\\int_0^1 x^2 dx = \\frac{1}{3}$.\\n\\n\\end{document}"
    }
  },
  "compilation_workflow": {
    "basic_compilation": {
      "latex": "pdflatex document.tex",
      "bibtex": "bibtex document",
      "makeindex": "makeindex document.idx",
      "final_latex": "pdflatex document.tex"
    },
    "automatic_tools": {
      "latexmk": "latexmk -pdf document.tex",
      "arbortext": "latexmk -pdf -pvc document.tex (continuous preview)"
    },
    "troubleshooting": {
      "missing_packages": "Check log file for missing package errors",
      "undefined_references": "Run pdflatex multiple times for cross-references",
      "bibliography_errors": "Ensure bibtex compilation between latex runs",
      "font_warnings": "Usually non-critical, can be suppressed with -interaction=nonstopmode"
    }
  },
  "version": "2.0",
  "last_updated": "2025-11-23",
  "compatibility": {
